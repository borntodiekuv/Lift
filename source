_G.Color = Color3.fromRGB(66, 135, 245)
_G.Color2 = Color3.fromRGB(66, 135, 245)

local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/slightten/ui-libs/main/funni"))()
local window = library:CreateWindow("Die Hub Lifting Simulator", Enum.KeyCode.RightControl)
local tab1 = window:CreateTab("Main")
local tab2 = window:CreateTab("Misc")
local sect2 = tab2:CreateSector("Misc", 'left')
sect2:AddLabel("V3rm borntodie1")

--Game Data
local Weights = {}
for i,v in pairs(game:GetService("ReplicatedStorage").Weights:GetChildren()) do
   table.insert(Weights,tostring(v))
end

local Chi = {}
for i,v in pairs(game:GetService("Players").LocalPlayer.PlayerGui.Main.Pages.Chi.Main.ChiList:GetChildren()) do
   table.insert(Chi,tostring(v))
end

local Classes = {}
for i,v in pairs((game:GetService("ReplicatedStorage").GameAssets.OnFireParticles:GetChildren())) do
   table.insert(Classes,tostring(v))
end

local Dispensers = {}

for i,v in pairs(workspace.Areas:GetDescendants()) do
    if v:IsA"Model" and v.Parent.Name == "Dispensers" then
        table.insert(Dispensers,tostring(v))
    end
end

local Workspace = game:GetService("Workspace")
local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Sell = Workspace.Areas.Valley.Pads.Sell.Part
local Player = Players.LocalPlayer
local Data = Player.PlayerScripts.LocalHandler.ReturnMod:Invoke("ReplicatedData")
local Chi = require(ReplicatedStorage.Chi)
local Training = ReplicatedStorage.Events.Training

-- farm

local sect1 = tab1:CreateSector("Farming", 'left')

sect1:AddToggle("Auto Strength", false, function(b)
    bb = b
    while bb do task.wait()
    game:GetService("ReplicatedStorage").Events.Training:FireServer("Lift")
    end
    end)

    sect1:AddToggle("Auto Sell", false, function(c)
        cc = c
        while cc do task.wait()
            for i,v in pairs(game:GetService("Workspace").Areas.Valley.Pads.Sell:GetDescendants()) do
                if v:IsA("TouchTransmitter") then
                firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, v.Parent, 0)
                wait()
                firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, v.Parent, 1)
                end
            end
        end
    end)

   
        sect1:AddToggle("Auto Sell at max capacity", false, function(d)
        dd = d
        while dd do task.wait()
        if Data.get("Strength") >=Chi.getChiInfo(Data.get("curChi")).capacity then
            firetouchinterest(Player.Character.HumanoidRootPart, Sell, 0)
            task.wait()
            firetouchinterest(Player.Character.HumanoidRootPart, Sell, 1)
            task.wait()
        end
        end
        end)
            
        


sect1:AddToggle("Auto Buy Weight", false, function(e)
    ee = e
        while ee do task.wait(5)
        
            for i = 1,#Weights do
                
                local ohString1 = Weights[i]
                
                game:GetService("ReplicatedStorage").Events.BuyWeight:FireServer(ohString1)
          
            end
        end
    end)

    sect1:AddToggle("Auto Buy Chi", false, function(e)
        ee = e
            while ee do task.wait(5)
            
                for i = 1,#Chi do
                    local ohString1 = Chi[i]
                    
                    game:GetService("ReplicatedStorage").Events.BuyChi:FireServer(ohString1)
              
                end
            end
        end)


        sect1:AddToggle("Auto Buy Class", false, function(e)
            ee = e
                while ee do task.wait(5)
                
                    for i = 1,#Classes do
                        local ohString1 = Classes[i]
                        
                        game:GetService("ReplicatedStorage").Events.BuyClass:FireServer(ohString1)
                  
                    end
                end
            end)

            local sect2 = tab1:CreateSector("Dispensers", 'right')

    
              local Dispenser =  sect2:AddDropdown("Select Dispenser",Dispensers, 'DragonStarDispenser', false, function(selected)
                getgenv().selectedDispensers = selected
            end)
            
        
            
            sect2:AddToggle("Auto Buy Dispenser", false, function(a)
                aa = a
            while aa do task.wait()
                local ohString1 = selectedDispensers
            game:GetService("ReplicatedStorage").Events.Hatch:FireServer(tostring(ohString1))
            end
            end)
         
            sect1:AddButton("Hide Name", function()                 
                for i,v in next, game.Players.LocalPlayer.Character.Head:GetChildren() do
                    if v:IsA"BillboardGui" then
                        v:Destroy()
                    end
                    end
                end)
