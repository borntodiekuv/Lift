for i,v in next, game.CoreGui:GetChildren() do
    if v.Name == "ScreenGui" and v.ImageButton then
        v:Destroy()
    end
    end

local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/sannin9000/Ui-Libraries/main/uwuware", true))()

local Window = Library:CreateWindow"Anime Lifting"

Window:AddButton({text = "Hide Name", flag = "button", callback = function()
for i,v in next, game.Players.LocalPlayer.Character.Head:GetChildren() do
    if v:IsA"BillboardGui" then
        v:Destroy()
    end
    end
end})

local egglist = {
    "DragonStarDispenser",
    "ShinobiStarDispenser",
    "PirateStarDispenser",
    "DemonStarDispenser"}

Window:AddList({text = "Eggs", flag = "list", values = egglist, callback = function(elist)
getgenv().eggopen = elist
end})

Window:AddToggle({text = "Auto Open Egg", flag = "toggle", state = false, callback = function(a)
aa = a
while aa do task.wait()
game:GetService("ReplicatedStorage").Events.Hatch:FireServer(tostring(eggopen))
end
end})

Window:AddToggle({text = "Auto Weight", flag = "toggle", state = false, callback = function(b)
bb = b
while bb do task.wait()
game:GetService("ReplicatedStorage").Events.Training:FireServer("Lift")
end
end})

Window:AddToggle({text = "Auto Sell", flag = "toggle", state = false, callback = function(c)
    cc = c
    while cc do task.wait()
        for i,v in pairs(game:GetService("Workspace").Areas.Valley.Pads.Sell:GetDescendants()) do
            if v:IsA("TouchTransmitter") then
            firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, v.Parent, 0)
            wait()
            firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, v.Parent, 1)
            end
        end
    end
end})
    
local Workspace = game:GetService("Workspace")
local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Sell = Workspace.Areas.Valley.Pads.Sell.Part
local Player = Players.LocalPlayer
local Data = Player.PlayerScripts.LocalHandler.ReturnMod:Invoke("ReplicatedData")
local Chi = require(ReplicatedStorage.Chi)
local Training = ReplicatedStorage.Events.Training

Window:AddToggle({text = "Auto Sell at Max Capacity", flag = "toggle", state = false, callback = function(d)
dd = d
while dd do task.wait()
if Data.get("Strength") >=Chi.getChiInfo(Data.get("curChi")).capacity then
    firetouchinterest(Player.Character.HumanoidRootPart, Sell, 0)
    firetouchinterest(Player.Character.HumanoidRootPart, Sell, 1)
end
end
end})
    
Window:AddFolder"Credits"

Window:AddLabel({text = "Made by Borntodie1 on v3rm"})

Library:Init()
